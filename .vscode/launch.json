    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8000/firebase/parse-and-send",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/boilermate_project_fb/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        }
    ]
}